{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "smarticafe-pro",
  "version": "0.1.0",
  "identifier": "com.smarticafe.pro",
  "build": {
    "beforeDevCommand": "echo bypass",
    "devUrl": "http://localhost:32520",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "创新意 电竞馆 - Smarticafe Pro",
        "width": 1440,
        "height": 900,
        "minWidth": 1280,
        "minHeight": 800
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
